{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "transactions": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "state": {
          "type": "string"
        },
        "start_time": {
          "type": "string",
          "format": "date-time"
        },
        "last_update_time": {
          "type": "string",
          "format": "date-time"
        },
        "expiration_time": {
          "type": "string",
          "format": "date-time"
        },
        "client_id": {
          "type": "string"
        },
        "node": {
          "type": "string"
        },
        "bucket": {
          "type": "string"
        },
        "scope": {
          "type": "string"
        },
        "collection": {
          "type": "string"
        },
        "operations": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string"
              },
              "key": {
                "type": "string"
              },
              "status": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}