{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "indexes": {
      "type": "object",
      "properties": {
        "datastore_id": {
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "index_key": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "is_primary": {
          "type": "boolean"
        },
        "keyspace_id": {
          "type": "string"
        },
        "metadata": {
          "type": "object",
          "properties": {
            "last_scan_time": {
              "type": "string"
            },
            "num_replica": {
              "type": "integer"
            },
            "stats": {
              "type": "object",
              "properties": {
                "last_known_scan_time": {
                  "type": "integer"
                }
              }
            }
          }
        },
        "name": {
          "type": "string"
        },
        "namespace": {
          "type": "string"
        },
        "namespace_id": {
          "type": "string"
        },
        "state": {
          "type": "string"
        },
        "using": {
          "type": "string"
        },
        "bucket_id": {
          "type": "string"
        },
        "scope_id": {
          "type": "string"
        }
      }
    }
  }
}