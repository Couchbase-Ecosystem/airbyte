{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "bucket": {
      "type": "string"
    },
    "scope": {
      "type": "string"
    },
    "collection": {
      "type": "string"
    },
    "content": {
      "type": "object",
      "additionalProperties": true
    },
    "metadata": {
      "type": "object",
      "properties": {
        "expiration": {
          "type": ["integer", "null"]
        },
        "flags": {
          "type": "integer"
        },
        "cas": {
          "type": "string"
        }
      }
    }
  }
}